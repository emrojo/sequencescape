<%= page_title "Editing activity" %>

<%= form_for(@activity) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => "activity_form", :locals => {:activity => @activity, :f => f } %>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @activity %> |
<%= link_to 'Back', activities_path %>
